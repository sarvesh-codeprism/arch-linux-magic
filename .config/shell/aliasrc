alias ytbdl="youtube-dl -f bestvideo+bestaudio -o '%(title)s.%(ext)s'"
alias ytadl="youtube-dl -f bestaudio -o '%(title)s.%(ext)s'"
alias ytdl="youtube-dl -f best -o '%(title)s.%(ext)s'"
alias ytpdl="youtube-dl -f best -o '%(playlist_index)s. %(title)s.%(ext)s'" 
alias syncsite="rsync -rtvzP ~/bwpw/public/* root@bugswriter.com:/var/www/site"
alias dots='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias hst="history | cut -c 8- | sort | uniq | fzf | tr '\\n' ' ' | xclip -sel c"
